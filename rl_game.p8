function _init()
	t=0
	p_ani={240,241,242,243}
	dirx,diry={-1,1,0,0},{0,0,-1,1}
	_upd=update_game
	_drw=draw_game
	startgame()
end

function _update60()
	t+=1
 _upd()
end

function _draw()
	_drw()
	drawind()
end

function startgame()
	btnbuff=-1
	p_x,p_y=1,1
	p_ox,p_oy=0,0
	p_sox,p_soy=0,0
	p_t=0
	p_flip=false
	p_mov=nil
	
	wind={}
	

end
