--gameplay
function moveplayer(dx,dy)
	local destx,desty=p_x+dx,p_y+dy	
	local tle=mget(destx,desty)
	
		if dx<0 then
			p_flip=true
		elseif dx>0 then
			p_flip=false
		end	
	
	if fget(tle,0) then
	 --wall bump
		p_sox,p_soy=dx*8,dy*8
		p_ox,p_oy=0,0
		p_t=0
		_upd=update_pturn
		p_mov=mov_bump
		if fget(tle,1) then
		 trig_bump(tle,destx,desty)
		end
	else
		--move animation
		sfx(63)
		p_x+=dx
		p_y+=dy
		
		p_sox,p_soy=-dx*8,-dy*8
		p_ox,p_oy=p_sox,p_soy
		p_t=0
		_upd=update_pturn
		p_mov=mov_walk
	end
end

function trig_bump(tle,destx,desty)
	if tle==7 or tle==8 then
		--vase
		mset(destx,desty,1)
		sfx(59)
	elseif tle==10 or tle==12 then
		--chests
		sfx(61)
		mset(destx,desty,tle-1)
	elseif tle==13 then
		--door
		sfx(62)
		mset(destx,desty,1)
	elseif tle==6 then
	 	addwind(32,64,64,24,{"welcome to the", "dungeon"})
	end
							
end
